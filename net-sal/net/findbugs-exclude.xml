<FindBugsFilter>
    <Match>
        <Class name="~.*_$" />
        <Bug code="UuF,UwF" />
    </Match>
    <Match>
        <Class name="org.opendaylight.net.ctl.of.impl.TraceManager$Worker" />
        <Bug code="NN" />
    </Match>
    <Match>
        <Class name="~com\.hp\.vxlan\.netconf.*" />
    </Match>
    <Match>
        <Class name="org.opendaylight.net.ctl.linkdisco.impl.LinkManager" />
    </Match>
    <Match>
        <Class name="~com\.hp\.sdn\.ctl\.topo\.impl.*" />
    </Match>
</FindBugsFilter>
