[project]
	description = OpenFlow Plugin
[receive]
	requireSignedOffBy = true
[access "refs/*"]
	owner = group ldap/gerrit-openflowplugin-commiters
	read = group GitHub Replication
[allowSuperproject "docs"]
	matching = refs/heads/stable/nitrogen:refs/heads/stable/nitrogen
[allowSuperproject "releng/autorelease"]
	matching = refs/heads/stable/lithium:refs/heads/stable/lithium
